#: src/guesser.c:22
#, c-format
msgid "Choose a random number between 1 and 100.\n"
msgstr "Выберите число от 1 до 100.\n"

#: src/guesser.c:26
#, c-format
msgid "Is the number greater than %d? (Yes/No): "
msgstr "Выбранное число больше, чем %d? (Yes/No): "

#: src/guesser.c:29
#, c-format
msgid "Error: failed to read input.\n"
msgstr "Ошибка: проблемы с обработкой пользовательского ввода.\n"

#: src/guesser.c:35
msgid "Yes"
msgstr "Yes"

#: src/guesser.c:37
msgid "No"
msgstr "No"

#: src/guesser.c:40
#, c-format
msgid "Invalid input. Please type exactly 'Yes' or 'No'.\n"
msgstr "Неверный ответ. Введите строго 'Yes' или 'No'.\n"

#: src/guesser.c:46
#, c-format
msgid "The number is %d!\n"
msgstr "Выбранное число — %d!\n"
